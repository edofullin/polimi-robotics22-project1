<launch>

<arg name="bag_file_name" value="bag2" />

<node pkg="rosbag" type="play" name="rosbag_play" args="--clock $(find project1)/bags/$(arg bag_file_name).bag --rate 0.4"/>
<node pkg="project1" type="vel_calc" name="vel_calc"/>
<node pkg="project1" type="odom" name="odom" output="screen"/>

</launch>